{
    "name": "whitedigital-eu/settings-bundle",
    "description": "Settings bundle",
    "type": "symfony-bundle",
    "keywords": [
        "api-platform",
        "php",
        "symfony",
        "symfony-bundle"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1.0",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*",
        "doctrine/persistence": "*",
        "symfony/config": ">=6.2",
        "symfony/dependency-injection": ">=6.2",
        "symfony/event-dispatcher": ">=6.2",
        "symfony/http-foundation": ">=6.2",
        "symfony/http-kernel": ">=6.2",
        "symfony/security-bundle": ">=6.2",
        "symfony/translation": ">=6.2",
        "whitedigital-eu/entity-resource-mapper-bundle": ">=0.16.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "roave/security-advisories": "dev-latest",
        "whitedigital-eu/config-pack": "*"
    },
    "autoload": {
        "psr-4": {
            "WhiteDigital\\SettingsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WhiteDigital\\SettingsBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        }
    }
}
